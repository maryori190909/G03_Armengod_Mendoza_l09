package graph;

public class VertexObj <V, E> {

    protected V info;
    protected int posi;

    public VertexObj(V info, int posi) {
        this.info = info;
        this.posi = posi;
    }

    public V getInfo() {
        return info;
    }

    public void setInfo(V info) {
        this.info = info;
    }

}

